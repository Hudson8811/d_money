                    #positioning
                        // buttons
                        .centerButtons
                            button#trade-button.btn-purple
                                | TRADE
                            button#autosellect-button.btn-orange
                                | AUTOSELECT
                        // end buttons
                        // bot filters
                        .bot-filters
                            p Bot filters
                        // end bot filters
                        // numbers
                        #numbers
                            p#one 0
                            span.nP -
                            p#two 1000
                        // end numbers
                        // type range
                        .range
                            section
                                span.tooltip
                                #slider4.slider.type2(oninput='funRange()')
                                span.volume
                        // end type range
                        // dash
                        .dash
                            .bg
                                p Character
                                img(src='images/plus.png')
                            .bg
                                p Rarity
                                img(src='images/arrowDown.png')
                            .bg
                                p Type
                                img(src='images/arrowDown.png')
                        // end dash
                        // board
                        .board
                            .switcher
                                // Rectangular switch
                                .switchElem
                                    label.switch
                                        input#rr3(type='checkbox' onchange='funSwitch3()')
                                        span.slider2.round
                                .switchElem
                                    label.switch
                                        input#rr4(type='checkbox' onchange='funSwitch4()')
                                        span.slider2.round
                            .checked
                                label.containerCheck
                                    input#pole3(type='radio')
                                    span.checkmark
                                label.containerCheck
                                    input#pole4(type='radio')
                                    span.checkmark
                            .names
                                p Exalted
                                p Inscribed
                        // end board
                        // more filters
                        .moreFilters.bg.dash
                            p More Filters
                            img(src='images/arrowDown.png')
                        .resetFilters
                            p RESET FILTERS
                        // /\/\more filters
                        .comissions
                            .comP Comissions
                            .comBoard
                                .item
