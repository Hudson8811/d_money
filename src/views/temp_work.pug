                                    .itemProcent
                                        p.bold 99%
                                        img(src='images/arrows.png')
                                        p.bold 106%
                                .item
                                    p.light Good:
                                    .itemProcent
                                        p.bold 99%
                                        img(src='images/arrows.png')
                                        p.bold 106%
                                .item
                                    p.light Skins:
                                    .itemProcent
                                        p.bold 99%
                                        img(src='images/arrows.png')
                                        p.bold 106%
                                .item
                                    p.light Misc:
                                    .itemProcent
                                        p.bold 99%
                                        img(src='images/arrows.png')
                                        p.bold 106%
