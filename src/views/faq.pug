extends layout-page

block styles
    link(rel="stylesheet", href="./stylesheets/faq.css")
    link(rel="stylesheet", href="./stylesheets/faq-media.css")
block content
    main
        .page-container
            .mainContainer
                .firstColumn
                    .about
                        .aboutHead
                            p About
                        .aboutCont
                            img(src="images/ico.png" alt="Icon")
                            p
                                | DOTA2MONEY is an online dota 2 trading bot, that lets you exchange your unwanted dota items.
                    .askedQuestions
                        .top
                            .frequently_asked_questions
                                | Frequently Asked Questions
                        .qa
                            .testbox
                                .question-box
                                    .box-q
                                        span Q
                                    .box-question
                                        p Why don&apos;t I see some of my items?
                                        i.material-icons.add add
                                .answer-box
                                    .box-a
                                        span A
                                    .box-answer
                                        p
                                            | Push the refresh button at your inventory. If the problem is not solved, try again in 1 minute. Most likely you have made a purchase on the market less then 7 days ago. Items appear only after 7 days after purchase.
                            .testbox
                                .question-box
                                    .box-q
                                        span Q
